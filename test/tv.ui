  <property name="styleSheet">
   <string notr="true">/* Styling for the Main Form */
QWidget#Form {
    background: qlineargradient(
        spread: pad, 
        x1: 0, y1: 0, x2: 1, y2: 0, 
        stop: 0 #d9fdd3, 
        stop: 1 #a8e6cf
    ); /* Gradient background */
    border: 1px solid #81c784; /* Subtle green border */
    border-radius: 20px; /* Rounded corners */
    padding: 20px; /* Inner spacing */
    box-shadow: 0px 10px 20px rgba(0, 0, 0, 0.1); /* Drop shadow */
}

/* Labels with a modern font and green text */
QLabel {
    color: #388e3c; /* Dark green text */
    font-size: 15px; /* Modern font size */
    font-weight: 600; /* Semi-bold text for clarity */
    margin-bottom: 8px; /* Spacing below labels */
}

/* Input fields with sleek borders and hover effects */
QLineEdit {
    background-color: #ffffff; /* White background */
    border: 2px solid #81c784; /* Subtle green border */
    border-radius: 10px; /* Rounded corners */
    padding: 8px 12px; /* Padding inside the input */
    font-size: 14px; /* Font size for inputs */
    color: #2e7d32; /* Dark green text */
    transition: border-color 0.3s ease, box-shadow 0.3s ease;
}

QLineEdit:focus {
    border: 2px solid #4caf50; /* Bright green border on focus */
    box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.15); /* Subtle glow on focus */
}

/* Buttons with modern gradients, hover, and pressed effects */
QPushButton {
    background: qlineargradient(
        spread: pad, 
        x1: 0, y1: 0, x2: 1, y2: 1, 
        stop: 0 #8bc34a, 
        stop: 1 #66bb6a
    ); /* Gradient green background */
    color: white; /* White text */
    border: none; /* No visible border */
    border-radius: 12px; /* Rounded corners */
    padding: 10px 20px; /* Comfortable padding */
    font-size: 15px; /* Font size for buttons */
    font-weight: 600; /* Bold text for buttons */
    box-shadow: 0px 6px 12px rgba(0, 0, 0, 0.1); /* Button shadow */
    transition: all 0.3s ease; /* Smooth animations */
}

QPushButton:hover {
    background: qlineargradient(
        spread: pad, 
        x1: 0, y1: 0, x2: 1, y2: 1, 
        stop: 0 #9ccc65, 
        stop: 1 #81c784
    ); /* Brighter gradient on hover */
    transform: translateY(-2px); /* Slight lift effect */
    box-shadow: 0px 8px 16px rgba(0, 0, 0, 0.2); /* Stronger shadow */
}

QPushButton:pressed {
    background: qlineargradient(
        spread: pad, 
        x1: 0, y1: 0, x2: 1, y2: 1, 
        stop: 0 #6e9d3c, 
        stop: 1 #4caf50
    ); /* Darker gradient on press */
    transform: scale(0.98); /* Slight shrink effect */
    box-shadow: 0px 2px 6px rgba(0, 0, 0, 0.1); /* Reduced shadow */
}

QPushButton:disabled {
    background-color: #c8e6c9; /* Muted green for disabled */
    color: #9e9e9e; /* Muted text */
    box-shadow: none; /* No shadow */
    transform: none; /* No animation */
}
</string>
